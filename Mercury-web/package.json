{
  "name": "mercury-cash",
  "version": "0.0.2",
  "description": "mercury app.",
  "scripts": {
    "=====DEVELOPMENT=====": "",
    "start": "npm run dev",
    "dev": "node --inspect bootstrap/server.development.js",
    "=====PRODUCTION=====": "",
    "bs": "npm run prod",
    "prod": "npm run bootstrap:prod",
    "bootstrap:prod": "npm run clean && npm run build:prod && npm run start:prod",
    "build:prod": "npm run build:prod:client && npm run build:prod:server",
    "build:prod:client": "webpack --config config/webpack/prod.client.webpack.js --profile --json > bootstrap/client-build/prod.client.stats.json",
    "build:prod:server": "webpack --config config/webpack/prod.server.webpack.js --profile --json > bootstrap/server-build/prod.server.stats.json",
    "start:prod": "node bootstrap/server.production.js",
    "========TOOLS=======": "",
    "clean": "rimraf bootstrap/client-build/ && rimraf bootstrap/server-build/ && mkdirp bootstrap/client-build && mkdirp bootstrap/server-build",
    "lint": "eslint client",
    "lint:errors": "eslint client --quiet",
    "lint:fix": "eslint client --fix"
  },
  "pre-commit": [
    "lint:errors"
  ],
  "author": "Calibraint Technologies <info@calibraint.com>",
